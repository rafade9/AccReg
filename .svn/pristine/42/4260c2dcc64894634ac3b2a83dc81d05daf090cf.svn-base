/**Estilos Redes **/
#face, #twit {
	position:fixed;
	z-index: 100;
	left:0px;
	border-radius:0px 15px 15px 0px;
	width:20px;
	height:33px;
	text-align: center;
	margin: 5px 0px;
}
#face {
	background:#3b5998 url(/wps/themes/html/YastasOrg/img/faceCompartamos.png) no-repeat center right;
	top:35%;
}
#twit {
	background:#00c0e3 url(/wps/themes/html/YastasOrg/img/twitCompartamos.png) no-repeat center right;
	top:40%;
}
#face:hover, #twit:hover, #face img, #twit img {
	width: 33px;
}
#face, #twit {
	transition:all .15s linear 0.125s;
	-moz-transition:all .15s linear 0.125s;
	-ms-transition:all .15s linear 0.125s;
	-o-transition:all .15s linear 0.125s;
	-webkit-transition:all .15s linear 0.125s;
	-khtml-transition:all .15s linear 0.125s;
}
#face a, #twit a {
	display:block;
	width:100%;
	height:100%;
}
